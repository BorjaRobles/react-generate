{
  "name": "cypress-generate",
  "description": "Generator for cypress pageObjects sharedcontext and specs",
  "version": "0.1.0",
  "main": "index.js",
  "bin": {
    "reactgen": "./index.js"
  },
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "generate:pageobject": "node index.js --pageobject",
    "generate:spec": "node index.js --spec"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/cornerjob/react-generate.git"
  },
  "keywords": [
    "cypress",
    "generators",
    "cli"
  ],
  "author": "borja",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/cornerjob/react-generate/issues"
  },
  "homepage": "https://github.com/cornerjob/react-generate",
  "reactGenerate": {
    "srcPath": "./cypress",
    "testFileExtension": "test"
  },
  "dependencies": {
    "app-root-path": "2.0.1",
    "chalk": "2.0.1",
    "commander": "2.11.0",
    "plop": "1.8.0"
  }
}
