{
  "name": "react-generate",
  "description": "Cornerjob react components, containers, domains generator",
  "version": "0.5.0",
  "main": "index.js",
  "bin": {
    "reactgen": "./index.js"
  },
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "generate:domain": "node index.js --domains",
    "generate:container": "node index.js --container",
    "generate:component": "node index.js --component"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/cornerjob/react-generate.git"
  },
  "keywords": [
    "react",
    "generators",
    "cli"
  ],
  "author": "cornerjob",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/cornerjob/react-generate/issues"
  },
  "homepage": "https://github.com/cornerjob/react-generate",
  "reactGenerate": {
    "srcPath": "./src",
    "testFileExtension": "test"
  },
  "dependencies": {
    "app-root-path": "2.0.1",
    "chalk": "2.0.1",
    "commander": "2.11.0",
    "plop": "1.8.0"
  }
}
